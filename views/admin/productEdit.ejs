            <main>
                <div class="container-fluid px-4">
                    <!-- <h1 class="mt-4">Dashboard</h1>
                    <ol class="breadcrumb mb-4">
                        <li class="breadcrumb-item active">Dashboard</li>
                    </ol> -->
                    

                    <section>
                        <div class="container">
                            <div class="row mt-5">
                                <div class="col-md-12">
                                    <h2 class="text-center">Edit product</h2>
                    
                                    <form  action="/admin/product/edit" method="post" enctype="multipart/form-data">
                                        <label for="">name</label>
                                        <input type="text" class="form-control" name="name" value="<%=productData[0].name%>">
                    
                                        <!-- <div class="form-group col-md-4"> -->
                                            <label for="inputState">Category</label>
                                            <select id="inputState" class="form-control" name="category">
                                            <option ><%=productData[0].catData[0].name%></option>
                                            <% categoryData.forEach((data)=>{ %> 

                                             <option><%=data.name%></option> 
                                             <%})%>
                                            </select>
                                        <!-- </div>   -->

                                        <label for="">Brand</label>
                                        <input type="text" class="form-control" name="brand" value="<%=productData[0].brand%>">
                    
                                        <label for="">Description</label>
                                        <input type="text" class="form-control" name="description" value="<%=productData[0].description%>">


                                        <label for="">Stock</label>
                                        <input type="text" class="form-control" name="stock" value="<%=productData[0].stock%>">
                    
                                        <label for="">price</label>
                                        <input type="text" class="form-control" name="price" value="<%=productData[0].price%>">
                    
                                        <label for="">image</label>
                                        <img src="/public/admin/assets/products/<%=productData[0].image%>" alt="" style="width: 5rem">
                                        <input type="file" class="form-control" name="image">
                    
                                        <input type="submit" value="submit" class="form-control btn btn-success">
                    
                                    </form>
                    
                                </div>
                            </div>
                        </div>
                    </section>





                </div>
            </main>
